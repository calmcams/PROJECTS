#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
	int min=1, max=100, num, n;
	srand (time(NULL));
	n = (rand() % 100) + 1;
	
	do{
	printf("Advinhe o numero, esta entre %d e %d\n", min, max);
	scanf("%d", &num);
	if (num<max && num>n)
	max=num;
	else if (num>min && num<n)
	min=num;	
	else if (num==n);
}
	while (num!=n);
	
	printf("PARABENS! Voce acertou o numero\n");
	}
